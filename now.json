{
  "name": "moe-node",
  "version": 2,
  "alias": "moe-node.northhighland.now.sh",
  "builds": [
    { "src": "routes/**/*.ts", "use": "@now/node" },
    { "src": "static/**", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/api(.*)", "dest": "/routes$1" },
    {
      "src": "(.*)",
      "dest": "/static$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ]
}
