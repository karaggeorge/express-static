{
  "name": "moe-node",
  "version": "0.0.1",
  "description": "Node.js server",
  "main": "server.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node .",
    "start:prod": "node dist/server.js",
    "build": "tsc -p .",
    "test": "xo && ava"
  },
  "dependencies": {
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "url-parse": "^1.5.9"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.11.3",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "ava": "^1.3.1",
    "eslint-config-xo-typescript": "^0.8.0",
    "sinon": "^7.3.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.0.3",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3333",
    "xo": "^0.24.0"
  },
  "xo": {
    "ignores": [
      "dist/**",
      "tests/**"
    ],
    "extends": [
      "xo-typescript"
    ],
    "extensions": [
      "ts"
    ],
    "space": 2,
    "rules": {
      "@typescript-eslint/indent": "off",
      "ava/no-ignored-test-files": "off",
      "@typescript-eslint/no-require-imports": "off",
      "import/first": "off"
    }
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
